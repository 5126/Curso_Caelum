class Funcionario{

	//atributos
	String  nome;
	String  departamento;
	double  salario;
	String  dataEntrada;
	String  rg;
	boolean estaNaEmpresa; 

	//metodos
	
	void recebeAumento(double aumento){

		aumento = salario + aumento;
	}

	double calculaGanhoAnual(){

		return salario*12;
		
	}

	void demite(){
	
	}

	void mostra(){
		System.out.println("Nome" + nome);
	
	}

}
